# Development Environment
# Используется при запуске npm run dev

# API URL для development (backend на localhost)
VITE_API_BASE_URL=http://localhost:8080

# Увеличенный таймаут для отладки
VITE_API_TIMEOUT=30000

# Включить debug логи
VITE_DEBUG=true

# Название приложения
VITE_APP_NAME=Activity Manager (Dev)

# Enable source maps
VITE_SOURCE_MAPS=true